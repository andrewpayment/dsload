{
  "name": "ds-load",
  "version": "1.0.0",
  "description": "Dominion Systems API load testing.",
  "main": "index.js",
  "repository": "git@github.com:andrewpayment/dsload.git",
  "author": "Andrew Payment <andrew.payment@dominionsystems.com>",
  "license": "MIT",
  "scripts": {
    "check-types": "tsc",
    "check-types:watch": "tsc --watch",
    "monitors": "docker compose up -d influxdb grafana",
    "build": "webpack",
    "go:tests:soak": "yarn build && k6 run dist/soakTest.bundle.js",
    "go:tests:userinfo": "yarn build && k6 run dist/getUsersTest.bundle.js",
    "go:tests:employeeSearch": "yarn build && k6 run dist/employeeSearchTest.bundle.js",
    "go:tests:getEmployeeTimePolicyConfiguration": "yarn build && k6 run dist/getEmployeeTimePolicyConfiguration.bundle.js",
    "docker": "yarn build && yarn docker:tests:soak",
    "docker:tests:soak": "docker compose run --rm k6 run /scripts/soakTest.bundle.js",
    "docker:tests:userinfo": "docker compose run --rm k6 run /scripts/getUsersTest.bundle.js",
    "docker:tests:employeeSearch": "docker compose run --rm k6 run /scripts/employeeSearchTest.bundle.js",
    "docker:tests:getEmployeeTimePolicyConfiguration": "docker compose run --rm k6 run /scripts/getEmployeeTimePolicyConfiguration.bundle.js",
    "docker:userinfo": "yarn build && yarn docker:tests:userinfo"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-transform-block-scoping": "^7.15.3",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/runtime": "^7.15.4",
    "@babel/runtime-corejs3": "^7.15.4",
    "babel-loader": "^8.2.2",
    "core-js": "^3.18.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "prettier": "^2.4.1",
    "typescript": "^4.4.3",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "@types/k6": "^0.32.2",
    "@types/node": "^16.9.6"
  }
}
